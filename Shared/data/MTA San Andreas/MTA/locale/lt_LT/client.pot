#: Client/loader/MainFunctions.cpp:798
#, c-format
msgid "Load failed. Could not find gta_sa.exe in %s."
msgstr "Įkėlimas nepavyko. Nerastas gta_sa.exe nurodytoje vietoje %s."

#: Client/loader/MainFunctions.cpp:808
#, c-format
msgid "Load failed. %s exists in the GTA directory. Please delete before continuing."
msgstr "Įkėlimas nepavyko. %s egzistuoja GTA direktorijoje. Prašome ištrinti prieš tęsiant."

#: Client/loader/MainFunctions.cpp:817
#, c-format
msgid "Main file has an incorrect name (%s)"
msgstr "Pagrindinis failas yra blogai užvadintas (%s)"

#: Client/loader/MainFunctions.cpp:828
msgid "Main file is unsigned. Possible virus activity.\n\n"
"See online help if MTA does not work correctly."
msgstr "Pagrindinis failas yra nepatvirtintas. Galimas virusų aktyvumas.\n\n"
"Peržiūrėkite pagalbą internete, jei MTA neveikia."

#: Client/loader/MainFunctions.cpp:851
msgid "Data files modified. Possible virus activity.\n\n"
"See online help if MTA does not work correctly."
msgstr "Data failai pakeisti. Galimas virusų aktyvumas.\n\n"
"Peržiūrėkite pagalbą internete, jei MTA neveikia."

#: Client/loader/MainFunctions.cpp:863
msgid ".asi files are in the 'MTA:SA' or 'GTA: San Andreas' installation directory.\n\n"
"Remove these .asi files if you experience problems with MTA:SA."
msgstr ".asi failai aptikti 'MTA:SA' arba 'GTA: San Andreas' aplankale.\n\n"
"Ištrinkite .asi failus jei patirsite nesklandumų su MTA:SA."

#: Client/loader/MainFunctions.cpp:967
msgid "File version mismatch error. Reinstall MTA:SA if you experience problems.\n"
msgstr "Neteisingos failo versijos klaida. Perrašykite MTA:SA, jei patirsite nesklandumų.\n"

#: Client/loader/MainFunctions.cpp:976
msgid "Some files are missing. Reinstall MTA:SA if you experience problems.\n"
msgstr "Trūksta kai kurių failų. Perrašykite MTA:SA, jei patirsite nesklandumų.\n"

#: Client/loader/MainFunctions.cpp:988
msgid "MTA:SA is not compatible with Windows 'Safe Mode'.\n\n"
"Please restart your PC.\n"
msgstr "MTA:SA yra nepalaikomas su Windows 'Safe Mode'.\n\n"
"Perkraukite savo PC.\n"

