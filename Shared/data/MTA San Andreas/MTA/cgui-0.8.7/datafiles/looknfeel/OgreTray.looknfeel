                    </Dim>
                </Area>
                <Image component="LeftEdge" name="OgreTrayImages/HorzTrackL"/>
                <Image component="RightEdge" name="OgreTrayImages/HorzTrackR"/>
                <Image component="Background" name="OgreTrayImages/HorzTrackM"/>
            </FrameComponent>
        </ImagerySection>
        <StateImagery name="Enabled">
            <Layer>
                <Section section="main"/>
            </Layer>
        </StateImagery>
        <StateImagery name="Disabled">
            <Layer>
                <Section section="main"/>
            </Layer>
        </StateImagery>
    </WidgetLook>
    <!--
:::: Shared static widget components.
-->
    <WidgetLook name="OgreTray/StaticShared">
        <ImagerySection name="frame">
            <FrameComponent>
                <Area>
                    <Dim type="LeftEdge">
                        <AbsoluteDim value="0"/>
                    </Dim>
                    <Dim type="TopEdge">
                        <AbsoluteDim value="0"/>
                    </Dim>
                    <Dim type="RightEdge">
                        <UnifiedDim scale="1" type="RightEdge"/>
                    </Dim>
                    <Dim type="BottomEdge">
                        <UnifiedDim scale="1" type="BottomEdge"/>
                    </Dim>
                </Area>
                <Image component="TopLeftCorner" name="OgreTrayImages/TextBoxTL"/>
                <Image component="TopRightCorner" name="OgreTrayImages/TextBoxTR"/>
                <Image component="BottomLeftCorner" name="OgreTrayImages/TextBoxBL"/>
                <Image component="BottomRightCorner" name="OgreTrayImages/TextBoxBR"/>
                <Image component="LeftEdge" name="OgreTrayImages/TextBoxL"/>
                <Image component="TopEdge" name="OgreTrayImages/TextBoxT"/>
                <Image component="RightEdge" name="OgreTrayImages/TextBoxR"/>
                <Image component="BottomEdge" name="OgreTrayImages/TextBoxB"/>
            </FrameComponent>
        </ImagerySection>
        <ImagerySection name="with_frame_background">
            <ImageryComponent>
                <Area>
                    <Dim type="LeftEdge">
                        <ImageDim name="OgreTrayImages/TextBoxL" dimension="Width"/>
                    </Dim>
                    <Dim type="TopEdge">
                        <ImageDim name="OgreTrayImages/TextBoxT" dimension="Height"/>
                    </Dim>
                    <Dim type="RightEdge">
                        <OperatorDim op="Subtract">
                            <UnifiedDim scale="1.0" type="RightEdge"/>
                            <ImageDim name="OgreTrayImages/TextBoxR" dimension="Width"/>
                        </OperatorDim>
                    </Dim>
                    <Dim type="BottomEdge">
                        <OperatorDim op="Subtract">
                            <UnifiedDim scale="1.0" type="BottomEdge"/>
