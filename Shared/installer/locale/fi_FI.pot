msgstr "Lisää Games Exploreriin"

#. INST_DIRECTX
#: Shared/installer/nightly.nsi:463
msgid "Install DirectX"
msgstr "Asenna DirectX"

#. INST_SEC_CLIENT
#: Shared/installer/nightly.nsi:497
msgid "Game client"
msgstr "Pelisovellus"

#. INST_SEC_SERVER
#: Shared/installer/nightly.nsi:498
